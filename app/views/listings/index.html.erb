<p id="notice"><%= notice %></p>

<h1>Listings</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Month</th>
      <th>Manufacturer</th>
      <th>Model</th>
      <th>Bodytype</th>
      <th>State</th>
      <th>Odometer</th>
      <th>Colour</th>
      <th>Condition</th>
      <th>Seats</th>
      <th>Doors</th>
      <th>Sold</th>
      <th>Price</th>
      <th>Description</th>
      <th>User</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.year %></td>
        <td><%= listing.month %></td>
        <td><%= listing.manufacturer %></td>
        <td><%= listing.model %></td>
        <td><%= listing.bodytype %></td>
        <td><%= listing.state %></td>
        <td><%= listing.odometer %></td>
        <td><%= listing.colour %></td>
        <td><%= listing.condition %></td>
        <td><%= listing.seats %></td>
        <td><%= listing.doors %></td>
        <td><%= listing.sold %></td>
        <td><%= listing.price %></td>
        <td><%= listing.description %></td>
        <td><%= listing.user_id %></td>
        <td><%= listing.category_id %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
